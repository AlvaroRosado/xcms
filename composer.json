{
	"name": "ximdex/ximdex",
    "description": "Ximdex CMS is a Semantic Content Management System (CMS)",
    "license": "AGPL",
    "type": "project",
    "keywords": ["cms", "semantic"],
    "homepage": "https://github.com/XIMDEX/ximdex",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "codemirror/Codemirror",
                "version": "4.7.0",
                "dist": {
                    "url": "https://github.com/codemirror/CodeMirror/archive/4.7.0.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "RyanMullins/angular-hammer",
                "version": "2.1.10",
                "dist": {
                    "url": "https://github.com/RyanMullins/angular-hammer/archive/v2.1.10.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "hammerjs/hammer.js",
                "version": "2.0.4",
                "dist": {
                    "url": "https://github.com/hammerjs/hammer.js/archive/2.0.4.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
    	"php": ">=7.1",
        "solarium/solarium": "3.8.*",
        "monolog/monolog": "1.23.*",
        "codemirror/Codemirror": "4.7.0",
        "RyanMullins/angular-hammer": "2.1.10",
        "hammerjs/hammer.js": "2.0.4",
        "pda/pheanstalk": "3.1.*",
        "symfony/console": "3.3.*",
        "php-di/php-di": "5.4.*",
        "doctrine/common": "2.8.*",
        "illuminate/support": "5.5.*",
        "phpseclib/phpseclib": "2.0.*"
    },
     "require-dev": {
        "mockery/mockery": "0.9.*",
        "codeception/codeception": "2.3.*"
    },
    "autoload": {
        "psr-0": { "Ximdex": "lib"}
    },
    "config": {
        "vendor-dir": "extensions/vendors/"
    }
}